@prefix biolink: <https://w3id.org/biolink/vocab/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix RO: <http://purl.obolibrary.org/obo/RO_> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix goldvocab: <https://w3id.org/gold.vocab/> .
@prefix mvikg: <http://example.org/mvikg/> .


# Custom MVIKG properties (both restrictions and annotations)
# mvikg:has_habitat can point to any GOLD biome (or their subclasses)
mvikg:has_habitat rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a sample to its environmental habitat from GOLD" ;
    rdfs:domain [ rdf:type owl:Class ;
                owl:unionOf ( sosa:Sample
			      biolink:OrganismTaxon
                            )
                ] ;
    rdfs:range mvikg:Biome ;
    rdfs:label "has habitat" .

# Define that mvikg:has_developmental_stage relates a sample to a developmental stage. Initially taken from UNIPROTRDFS, that definition did not fit here, hence removed.
mvikg:has_developmental_stage rdf:type owl:ObjectProperty ;
    rdfs:domain sosa:Sample ;
    rdfs:range mvikg:DevelopmentalStage ;
    rdfs:label "has developmental stage" .



# Define that has_phenotype relates genes to phenotypes
biolink:has_phenotype
    rdfs:domain biolink:Gene ;
    rdfs:range biolink:PhenotypicFeature .



# Define domains and ranges for SOSA properties used in cross-links
sosa:hasFeatureOfInterest
    rdfs:domain biolink:GenomicEntity ;
    rdfs:range sosa:Sample .



sosa:hasSample
    rdfs:domain biolink:OrganismTaxon ;
    rdfs:range sosa:Sample .

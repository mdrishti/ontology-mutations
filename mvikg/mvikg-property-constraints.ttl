@prefix biolink: <https://w3id.org/biolink/vocab/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix RO: <http://purl.obolibrary.org/obo/RO_> .
@prefix ENVO: <http://purl.obolibrary.org/obo/ENVO_> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix gold: <https://w3id.org/gold.path/> .

# Define that RO:hasHabitat can be used with sosa:Sample
# and can point to several GOLD biomes
gold:AnyBiome a owl:Class ;
    owl:unionOf ( gold:Biome_A gold:Biome_B gold:Biome_C ) .
RO:0002303 rdfs:range gold:AnyBiome .

#RO:0002303  # hasHabitat

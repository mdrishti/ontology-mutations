@prefix biolink: <https://w3id.org/biolink/vocab/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix RO: <http://purl.obolibrary.org/obo/RO_> .
@prefix mvikg: <http://example.org/mvikg/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .

# If a Sample has a habitat/location/biome, it CAN ONLY be a GOLD biome
# (i.e., one of: AtomicElement, Engineered, Environmental, Host-associated, or their subclasses)
sosa:Sample rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty mvikg:has_habitat ;
    owl:allValuesFrom mvikg:Biome
] .

# If a Sample has a developmental stage, it CAN ONLY be a mvikg:DevelopmentalStage
sosa:Sample rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty mvikg:has_developmental_stage ;
    owl:allValuesFrom mvikg:DevelopmentalStage
] .

# If a Sample is sample of something, it CAN ONLY be an OrganismTaxon
sosa:Sample rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sosa:isSampleOf ;
    owl:allValuesFrom biolink:OrganismTaxon
] .

# If a Sample is part of something, it CAN ONLY be a Dataset
sosa:Sample rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dcterms:isPartOf ;
    owl:allValuesFrom dcmitype:Dataset
] .

# If a Sample is feature of interest, it CAN ONLY be a GenomicEntity
sosa:Sample rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sosa:isFeatureOfInterestOf ;
    owl:allValuesFrom biolink:GenomicEntity
] .

# If a Gene has a phenotype, it CAN ONLY be a PhenotypicFeature
biolink:Gene rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty biolink:has_phenotype ;
    owl:allValuesFrom biolink:PhenotypicFeature
] .

# If a Gene has a sequence variant, it CAN ONLY be a SequenceVariant
biolink:Gene rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty biolink:has_sequence_variant ;
    owl:allValuesFrom biolink:SequenceVariant
] .

# If a Gene has a variant qualifier, it CAN ONLY be a VariantToGeneAssociation
biolink:Gene rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty biolink:form_or_variant_qualifier ;
    owl:allValuesFrom biolink:VariantToGeneAssociation
] .

# If a SequenceVariant has a gene, it CAN ONLY be a Gene
biolink:SequenceVariant rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty biolink:has_gene ;
    owl:allValuesFrom biolink:Gene
] .

# If a SequenceVariant is variant of something, it CAN ONLY be a Gene
biolink:SequenceVariant rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty biolink:is_sequence_variant_of ;
    owl:allValuesFrom biolink:Gene
] .

# If an OrganismTaxon has a sample, it CAN ONLY be a Sample
biolink:OrganismTaxon rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sosa:hasSample ;
    owl:allValuesFrom sosa:Sample
] .

# If a GenomicEntity has feature of interest, it CAN ONLY be a Sample
biolink:GenomicEntity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sosa:hasFeatureOfInterest ;
    owl:allValuesFrom sosa:Sample
] .
